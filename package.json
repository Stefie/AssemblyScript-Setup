{
  "dependencies": {
    "assemblyscript": "AssemblyScript/assemblyscript"
  },
  "scripts": {
    "build:untouched": "asc src/index.ts -b build/untouched.wasm -t build/untouched.wat --sourceMap --validate --debug",
    "build:optimized": "asc src/index.ts -b build/optimized.wasm -t build/optimized.wat --sourceMap --validate --optimize",
    "build": "yarn run build:untouched && yarn run build:optimized"
  }
}